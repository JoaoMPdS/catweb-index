{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "subdomains": {
            "type": "object",
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "path": {
                            "type": "string"
                        },
                        "title": {
                            "type": "string"
                        },
                        "description": {
                            "type": "string"
                        },
                        "iconId": {
                            "type": "integer"
                        }
                    },
                    "required": [
                        "path",
                        "title"
                    ]
                }
            }
        },
        "visits": {
            "type": "object",
            "properties": {
                "current": {
                    "type": "integer"
                },
                "updatedAt": {
                    "type": "string",
                    "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
                }
            },
            "required": [
                "current",
                "updatedAt"
            ]
        },
        "creatorId": {
            "type": "string"
        },
        "createdAt": {
            "type": "string",
            "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
        }
    },
    "required": [
        "subdomains",
        "visits",
        "creatorId",
        "createdAt"
    ]
}